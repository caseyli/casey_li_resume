<!DOCTYPE html>
<html>
<head>
  <title><%= title %></title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tag %>
  <%= render 'layouts/stylesheets' %>
</head>
<body>
	<div class="container">
		<div class="main">
		<%= render 'layouts/header'%>
		<%= yield %>
		</div>
		<div class="footer">
		<%= render 'layouts/footer' %>
		</div>
	</div>
</body>
</html>
