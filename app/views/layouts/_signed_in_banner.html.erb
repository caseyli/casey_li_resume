<div class='signed-in-banner'>
  <div class="container">

    <% if signed_in? %>
      Signed in!

      <% if admin? %>
        <%= link_to "Admin", admin_path %>
      <% end %>
      

      <%= link_to "Sign out", destroy_user_session_path, method: :delete %>
    <% else %>
      <%= link_to "Sign in", new_user_session_path %>
    <% end %>
  </div>
</div>